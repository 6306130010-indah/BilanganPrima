#include <iostream>

using namespace std;

int main()
{
    int angka;
    int test;
    int x;
    int i;


    cout << "masukkan angka awal : " << endl;
    cin >> angka;
    cout << "masukkan angka akhir : " << endl;
    cin >> angka;

    for (x = 2; x<=angka; x++){
    test = 0;
    for (i = 2; i<x; i++)
    {if (x % i == 0)
    {test = 1;
    }} if (test == 0)
    {
        {
            cout << x<<",";
        }}
        {
            cout << endl;
        }
    }
    return 0;
}
